<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSFG_AutoFarm"
        xmlns:Properties="clr-namespace:BSFG_AutoFarm.Properties" x:Class="BSFG_AutoFarm.Login"
        mc:Ignorable="d"
        Title="EW Walker Pro v1.0" Height="450" Width="567"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="12"
        FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto" ResizeMode="CanMinimize" Icon="images/walker.ico">
    <Grid>
        <Image Source="images/ew walker.png"/>
        <TextBox MaxLength="25" Padding="10,0,10,2" x:Name="textBox" HorizontalAlignment="Left" Height="35" Margin="160,85,0,0" TextWrapping="Wrap" Text="Введите ваш ключ" VerticalAlignment="Top" Width="240" Background="White" FontSize="14"/>
        <Button x:Name="button" Content="Войти" HorizontalAlignment="Left" Margin="220,195,0,0" VerticalAlignment="Top" Width="110" Click="button_Click" Background="#4CFFFFFF" Foreground="White"/>
    </Grid>

</Window>

